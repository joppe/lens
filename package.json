{
    "name": "lens",
    "version": "0.0.0",
    "description": "Set/get property values of an object",
    "main": "dist/index",
    "typings": "dist/index",
    "scripts": {
        "build": "npm run clean && node ./node_modules/.bin/tsc",
        "clean": "rm -rf dist",
        "lint": "eslint",
        "prepublishOnly": "npm run build",
        "test": "jest --coverage",
        "test:watch": "jest --watch"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/joppe/lens.git"
    },
    "importSort": {
        ".js, .jsx, .ts, .tsx": {
            "style": "module",
            "parser": "typescript"
        }
    },
    "author": {
        "name": "Joppe Aarts",
        "email": "joppe@apestaartje.info"
    },
    "license": "MIT",
    "keywords": [
        "TypeScript"
    ],
    "homepage": "https://github.com/joppe/lens#readme",
    "files": [
        "dist"
    ],
    "devDependencies": {
        "@types/jest": "^26.0.15",
        "@typescript-eslint/eslint-plugin": "^4.6.1",
        "@typescript-eslint/parser": "^4.6.1",
        "coveralls": "^3.1.0",
        "eslint": "^7.12.1",
        "eslint-config-prettier": "^6.15.0",
        "import-sort-style-module": "^6.0.0",
        "jest": "^26.6.3",
        "prettier": "^2.1.2",
        "prettier-plugin-import-sort": "0.0.6",
        "ts-jest": "^26.4.3",
        "typescript": "^4.0.5"
    }
}
